<table>

	<thead>
		<tr>
			<th>ジャンル検索</th>
		</tr>
	</thead>

	<% genres.each do |genre| %>

		<% if genre.is_active == true %> <!-- 有効時のみ表示 -->

			<tbody>
				<tr>
					<td>
						<%= link_to genre.name, public_items_genre_search_path(genre_id_search: genre.id) %>
					</td>
				</tr>
			</tbody>

		<% end %>

	<% end %>

</table>
